@model PFMS.ViewModels.ViewList

<!DOCTYPE html>

<!-- molla/checkout.html  22 Nov 2019 09:55:06 GMT -->
<head>
    <link href="~/assets/css/style.css" rel="stylesheet" type="text/css">
    <link href="~/assets/css/homestyle.css" rel="stylesheet" type="text/css">
    <meta http-equiv='cache-control' content='no-cache'>
    <meta http-equiv='expires' content='0'>
    <meta http-equiv='pragma' content='no-cache'>
    <meta charset="UTF-8">

</head>

<body>
    <div class="page-content">
        <div class="container" style="margin-top: 30px;">
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th style="text-align: center; vertical-align: middle;">موقع</th>
                            <th style="text-align: center; vertical-align: middle;">كود الحساب</th>
                            <th style="text-align: center; vertical-align: middle;">الحركه</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.psls)
                        {
                            <tr>
                                <td style="text-align: center; vertical-align: middle;"><label>@item.s_name</label></td>
                                <td style="text-align: center; vertical-align: middle;"><label>@item.p_id</label></td>
                                <td style="text-align: center; vertical-align: middle;"><label>@item.type</label></td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>

    </div><!-- End .page-wrapper -->
</body>


<!-- molla/checkout.html  22 Nov 2019 09:55:06 GMT -->
